<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:padding="9dp"    android:clickable="true"    android:focusable="true"    android:id="@+id/Music_layout"    android:background="?attr/selectableItemBackground">    <LinearLayout        android:id="@+id/userPlaylist_LinearLayout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/userPlaylist_icon"            android:layout_width="55dp"            android:layout_height="55dp"            app:srcCompat="@drawable/song_img"            app:shapeAppearance="@style/curved"/>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_marginEnd="40dp"            android:orientation="vertical">            <TextView                android:id="@+id/userPlaylist_foldrName"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:paddingEnd="9sp"                android:layout_marginTop="4dp"                android:layout_marginStart="10dp"                android:textColor="@color/PayneGrey"                android:maxLines="1"                android:textSize="16sp"                tools:ignore="RtlSymmetry" />            <TextView                android:id="@+id/userPlaylist_selectCount"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:textColor="@color/DarkGray"                android:textSize="14sp"                android:maxLines="1"                android:layout_marginTop="8dp"                android:layout_marginStart="10dp"/>        </LinearLayout>    </LinearLayout>        <ImageButton            android:id="@+id/userPlaylist_menu"            android:layout_width="40dp"            android:layout_alignParentEnd="true"            android:textSize="16sp"            android:layout_marginRight="-10dp"            android:background="@android:color/transparent"            android:src="@drawable/icons8_menu"            android:layout_height="55dp"            app:tint="@color/PayneGrey" /></RelativeLayout>