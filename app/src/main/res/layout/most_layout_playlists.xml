<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:app="http://schemas.android.com/apk/res-auto">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:padding="15dp"        android:clickable="true"        android:focusable="true"        android:id="@+id/Music_layout"        android:background="?attr/selectableItemBackground"        >        <LinearLayout            android:id="@+id/most_layout_LayoutView"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal">            <com.google.android.material.imageview.ShapeableImageView                android:id="@+id/most_layout_ShapeableImageView"                android:layout_width="50dp"                android:layout_height="50dp"                android:background="#00ACC1"                app:shapeAppearance="@style/curved"/>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_marginRight="40dp"                android:orientation="vertical">                <TextView                    android:id="@+id/most_layout_TextView1"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:paddingRight="9sp"                    android:layout_marginTop="4dp"                    android:layout_marginStart="13dp"                    android:textColor="#FFFFFF"                    android:maxLines="1"                    android:textSize="16dp"/>                <TextView                    android:id="@+id/most_layout_TextView2"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:textColor="@color/ic_launcher_background"                    android:textSize="12dp"                    android:maxLines="1"                    android:layout_marginTop="4dp"                    android:layout_marginStart="13dp"/>            </LinearLayout>        </LinearLayout>    </LinearLayout>    <RelativeLayout        android:layout_width="38dp"        android:layout_height="38dp"        android:layout_marginEnd="5dp"        android:layout_alignParentEnd="true"        android:layout_centerVertical="true"        >        <com.google.android.material.imageview.ShapeableImageView            android:layout_width="38dp"            android:layout_height="38dp"            android:background="?attr/selectableItemBackground"            android:clickable="true"            android:focusable="true"            android:id="@+id/most_layout_menu_ShapeableImageView"            app:shapeAppearance="@style/corner50" />        <ImageView            android:layout_width="24dp"            android:layout_height="24sp"            android:layout_centerInParent="true"            android:src="@drawable/dots2"            />    </RelativeLayout></RelativeLayout>